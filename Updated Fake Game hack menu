function disp1(){
    document.getElementById('submenu').classList.toggle("hide");
}

function disp(){
    if(document.getElementById('submenu').classList.value != "hide"){
        document.getElementById('submenu').classList.toggle("hide");
    }
    document.getElementById('menu').classList.toggle("hide");

}
